{
    "UseInMemoryDatabase": false,
    "AzureAd": {
        "Audiences": "api://dev.teamsapps.local:3000 <app registration Application ID URI> <app registration client id>",
        "ClientId": "<app registration client id>",
        "ClientSecret": "<app registration client secret>"
    },
    "KeyVaultName": "<azure key vault name>",
    "SharePointDomain": "<tenant domain>.sharepoint.com",
    "BlobStorage": {
        "Uri": "https://<account name>.blob.core.windows.net/",
        "AccountName": "<account name>"
    },
    "PnPCore": {
        "HttpRequests": {
            "UserAgent": "ISV|<tenant domain>|Sustain"
        }
    },
    "AllowedHosts": "*"
}